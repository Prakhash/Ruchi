<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ruchi.hibernate.model.DAO.RestaurantFoodDao" table="rest_food">
        <id name="rest_id" type="java.lang.String">
            <column name="rest_id" />
            <!--<generator class="increment" />-->
        </id>
        <property name="food_id" type="java.lang.String">
            <column name="food_id" />
        </property>
        <property name="rating" type="java.lang.Float">
            <column name="rating" />
        </property>
    </class>
</hibernate-mapping>